{
  "name": "byodianBlog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/assets/sass/main.scss src/assets/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass src/assets/sass/main.scss -o src/assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" src/assets/css/style.comp.css -o src/assets/css/style.prefix.css",
    "compress:css": "node-sass src/assets/css/style.prefix.css src/assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "11": "eleventy --serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "moment": "^2.24.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^6.1.3",
    "slugify": "^1.3.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "highlight.js": "^9.15.10"
  }
}
