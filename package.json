{
  "name": "byodianBlog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "11": "eleventy --serve --quiet",
    "watch:sass": "node-sass src/assets/scss/main.scss src/assets/css/style.css -w",
    "compile:sass": "node-sass src/assets/scss/main.scss -o src/assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" src/assets/css/style.comp.css -o src/assets/css/style.prefix.css",
    "compress:css": "node-sass src/assets/css/style.prefix.css src/assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "autoprefixer": "^7.1.4",
    "clean-webpack-plugin": "^3.0.0",
    "concat": "^1.0.3",
    "css-loader": "^3.2.0",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.24.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0"
  }
}
