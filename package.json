{
  "name": "byodian-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "del dist",
    "dev:postcss": "postcss src/_assets/stylesheets/*.css -o src/static/style.css --watch --verbose",
    "dev:scripts": "esbuild src/_assets/scripts/*.js --bundle --watch --outfile=src/static/app.js",
    "dev:11ty": "eleventy --serve --watch",
    "build:postcss": "postcss src/_assets/stylesheets/*.css -o src/static/style.css",
    "build:scripts": "esbuild src/_assets/scripts/*.js --bundle --minify --outfile=src/static/app.js",
    "build:11ty": "eleventy",
    "start": "npm-run-all clean --parallel dev:*",
    "build": "npm-run-all clean build:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "autoprefixer": "^10.4.7",
    "concat": "^1.0.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.16.0",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "esbuild": "^0.14.42"
  }
}
